@model IEnumerable<Group9_006_iCAREAPP.Models.DocMetaData>

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.DocName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DocCreationDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PatientRecord.TreatmentId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Worker.RoleId)
            </th>
            
            <th></th>
        </tr>
    
    @foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.DocName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DocCreationDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PatientRecord.TreatmentId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Worker.RoleId)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.DocId }) |
            @Html.ActionLink("Details", "Details", new { id = item.DocId }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.DocId }) 
 
        </td>
    </tr>
    }
    
    </table>
</body>
</html>
